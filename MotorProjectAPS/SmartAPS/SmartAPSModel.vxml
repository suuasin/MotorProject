                  <Commands>
                    <cdd:DataCommand Name="Cmd1" IsBindTable="True">
                      <CommandText>/* SELECT A.PROCESS_ID AS "PROCESS___PROCESS_ID",
B.PRODUCT_ID || ' &amp; ' || B.PROCESS_ID AS "PRODUCT___PRODUCT_ID__PROCESS_ID",
E.PRODUCT_ID || ' &amp; ' || E.PROCESS_ID AS "EQP_ARRANGE___PRODUCT_ID__PROCESS_ID",
E.PROCESS_ID || ' &amp; ' || E.STEP_ID AS "EQP_ARRANGE___PROCESS_ID__STEP_ID",
C.PROCESS_ID || ' &amp; ' || C.STEP_ID AS "STEP_ROUTE___PROCESS_ID__STEP_ID",
D.STD_STEP_ID AS "STD_STEP_INFO___STD_STEP_ID",
E.EQP_ID AS "EQP_ARRANGE___EQP_ID",
F.EQP_ID AS "EQUIPMENT___EQP_ID",
G.PRODUCT_ID AS "DEMAND___PRODUCT_ID"
FROM APSLIB.VW_PROCESS A
FULL JOIN APSLIB.VW_PRODUCT B
ON A.PROCESS_ID = B.PROCESS_ID
FULL JOIN APSLIB.VW_STEP_ROUTE C
ON A.PROCESS_ID = C.PROCESS_ID
FULL JOIN APSLIB.VW_STD_STEP_INFO D
ON C.STEP_ID = D.STD_STEP_ID
FULL JOIN APSLIB.VW_EQP_ARRANGE E
ON E.PRODUCT_ID = B.PRODUCT_ID
AND E.PROCESS_ID = B.PROCESS_ID
AND E.STEP_ID = C.STEP_ID
AND E.PROCESS_ID = C.PROCESS_ID
FULL JOIN APSLIB.VW_EQUIPMENT F
ON E.EQP_ID = F.EQP_ID
FULL JOIN (
SELECT PRODUCT_ID
FROM APSLIB.VW_DEMAND
GROUP BY PRODUCT_ID ) G
ON G.PRODUCT_ID = B.PRODUCT_ID
WHERE A.PROCESS_ID IS NULL OR B.PROCESS_ID IS NULL OR C.PROCESS_ID IS NULL OR D.STD_STEP_ID IS NULL OR C.STEP_ID IS NULL OR E.PRODUCT_ID IS NULL OR B.PRODUCT_ID IS NULL
OR E.EQP_ID IS NULL OR F.EQP_ID IS NULL OR E.PRODUCT_ID IS NULL OR E.PROCESS_ID IS NULL OR E.STEP_ID IS NULL OR G.PRODUCT_ID IS NULL                    

*/ </CommandText>
                    </cdd:DataCommand>
